@model List<IptProject.Models.FYP.StudentProposal>
@{
    ViewBag.Title = "GetProduct";
    Layout = "~/Views/Shared/_PortalLayout.cshtml";
}

<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-lg-3 col-md-6 col-sm-6">
            <a href="">

                <div class="card card-stats">

                    <div class="card-header card-header-warning card-header-icon">
                        <div class="card-icon">
                            <i class="material-icons">insert_link</i>
                        </div>
                        @*<p class="card-category">Used Space</p>*@
                        <h3 class="card-title">
                            @item.ProjectTitle

                        </h3>
                        <div class="tim-typo" style="color:black">
                            @Html.ActionLink("Open", "ViewStudentProposalSupervisor", new { title = item.ProjectTitle }, null)

                        </div>
                    </div>
                    <div class="card-footer">
                        <div class="stats">
                            @*<i class="material-icons text-success">money</i>*@
                            <a href="javascript:;"> </a>
                        </div>
                    </div>
                </div>
            </a>
        </div>
    }



</div>
@section scripts{
    <script type="text/javascript">
        function sweetalert() {
            swal('Please wait');
        }
    </script>



}

