@model IEnumerable<IptProject.Models.FYP.FYP2FinalEvaluation>

@{
    ViewBag.Title = "FinalEvaluation";
    Layout = "~/Views/Shared/_PortalLayout.cshtml";
}

<h2>FinalEvaluation</h2>
@* PROJECT CARDS *@
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-lg-3 col-md-6 col-sm-6">


            <div class="card card-stats">

                <div class="card-header card-header-warning card-header-icon">
                    <div class="card-icon">
                        <i class="material-icons">insert_link</i>
                    </div>
                    @*<p class="card-category">Used Space</p>*@
                    <h3 class="card-title">
                        @item.ProjectName

                    </h3>
                    <div class="tim-typo" style="color:black">
                        @Html.ActionLink("Open", "ViewFinalEvaluation", new { title = item.ProjectName }, null)
                    </div>
                </div>

            </div>

        </div>
    }
</div>
