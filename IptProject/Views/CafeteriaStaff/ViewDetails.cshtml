
@model List<IptProject.Models.Cafeteria.OrderDetails>

<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-header card-header-primary">
                <h4 class="card-title ">Order Details</h4>

            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table">
                        <thead class="text-primary">
                        <th>
                            Item Name
                        </th>
                        <th>
                            Quantity
                        </th>
                        </thead>
                        <tbody>
                            @foreach (var item in ViewBag.orderdetails)
                            {
                                <tr>
                                    <td>
                                        @item.Itemname
                                    </td>
                                    <td>
                                        @item.Quantity
                                    </td>

                                </tr>
                            }


                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>

</div>

